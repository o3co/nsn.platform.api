{
  presets: [
    "@babel/preset-flow",
    ["@babel/env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  'plugins': [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-syntax-export-namespace-from",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
  ]
}

