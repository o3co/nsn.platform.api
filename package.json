{
  "name": "@nsn/platform",
  "version": "0.1.2",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "repository": "git@github.com:o3co/nsn.platform.api.git",
  "author": "Yoshi Aoki <yoshi@1o1.co.jp>",
  "license": "MIT",
  "engines": {
    "node": ">=8.15.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:commonjs": "yarn rollup -c",
    "build:flow": "flow-copy-source src dist/module",
    "build:module": "babel --out-dir=dist/module src",
    "clean": "rimraf dist",
    "cover": "npm-run-all cover:*",
    "cover:flow": "flow-coverage-report --config .flow-coverage-reportrc.js",
    "cover:jest": "jest src --coverage",
    "flowcheck": "yarn flow src",
    "lint": "eslint src",
    "postversion": "git push && git push --tags",
    "prebuild": "npm-run-all clean lint flowcheck",
    "prebuild:clean": "yarn clean",
    "precommit": "npm-run-all flowcheck lint",
    "prepare": "npm-run-all build",
    "preversion": "npm-run-all lint",
    "test": "yarn jest src",
    "test:watch": "yarn jest src --watch --notify",
    "version": "npm-run-all build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:babel": "babel src --out-dir dist/node --watch --verbose",
    "watch:flow": "watch '$npm_execpath run lint:flow' ./src"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@nsn/eslint-config": "^0.1.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "debug": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "flow-bin": "^0.100.0",
    "flow-copy-source": "^2.0.2",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.2",
    "flow-watch": "^1.1.4",
    "fstream": ">=1.0.12",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1"
  }
}
